"%%"(L,R):(L*R/100.0);
(30 + 50 %% 40)@println;

"*" "%"(L,R):(L*R/100.0);
(30 + 50 % 40)@println;

"++"(X):(X+=1);
(i:0; i < 10) ?* (@println(i++));

"--"(X):(X-=1);
(i:10; i > 0) ?* (@println(--i));

"[+]"(L,R): (
	V:[];
	(X:L[..()]) ?* (
		V[V@length] = X + R
	);
	V
);
a:[1,2,3,4,5,6,7,8,9];
(a[+]10)@println;
