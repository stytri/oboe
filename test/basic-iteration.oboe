 i:10, (i >  0         ) ?* (@print_line i, i=(i-1));
(j:10;  j >  0         ) ?* (@print_line j, j=(j-1));
(k:10;  k >  0; k=(k-1)) ?*  @print_line k;
(k:10;  k == 0; k=(k-1)) !*  @print_line k;

(j:0; j < 10; j=(j+1)) ?* (
	(k:0; k < j; k=(k+1)) ?* ((
		@print_line (j, k)
	) ; (
		@print_line "no iteration"
	))
)
