a:[1,2,3];
b:[1,2,3];
c:[4,5,6];
d:[1,2,3,4,5,6];
e:[3,3,3,3,3,3];
expect(e,r,s): (
	(s, " expected ", e, " got ", r)@print,
	(e <> r) ? " *"@print,
	""@println
);
